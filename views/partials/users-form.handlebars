<fieldset>
  <h2>New Participant</h2>
  <div class="form-group">
    <p>
      <label id="user-label" for="first-name">First Name</label><br>
      <input id="first-name" type="text" name="firstName" value="{{user.firstName}}"/>
    </p>
  </div>

  <div class="form-group">
    <p>
      <label id="user-label" for="last-name">Last Name</label><br>
      <input id="last-name" type="text" name="lastName" value="{{user.lastName}}"/>
    </p>
  </div>


  <div class="form-group">
    <p>
      <label id="user-label" for="organization">Organization</label><br>
      <input id="organization" type="text" name="organization" value="{{user.organization}}"/>
    </p>
  </div>

  <div class="form-group">
    <p>
      <label id="user-label" for="emailAdress">Email Address</label><br>
      <input id="emailAdress" type="text" name="emailAdress" value="{{user.emailAdress}}"/>
    </p>
  </div>

  </fieldset>
