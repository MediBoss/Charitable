<fieldset>
<legend>New Participant</legend>
<div class="form-group">
  <p>
    <label for="first-name">First Name</label><br>
    <input id="first-name" type="text" name="firstName" value="{{user.firstName}}"/>
  </p>
</div>

<div class="form-group">
  <p>
    <label for="last-name">Last Name</label><br>
    <input id="last-name" type="text" name="lastName" value="{{user.lastName}}"/>
  </p>
</div>


<div class="form-group">
  <p>
    <label for="organization">Organization</label><br>
    <input id="organization" type="text" name="organization" value="{{user.organization}}"/>
  </p>
</div>

<div class="form-group">
  <p>
    <label for="emailAdress">Email Address</label><br>
    <input id="emailAdress" type="text" name="emailAdress" value="{{user.emailAdress}}"/>
  </p>
</div>

</fieldset>
